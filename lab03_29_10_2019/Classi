Server -> 
	serviziDiRicerca();

Client ->
	aggiungiImpiegato();
	modificaImpiegato();
	
SedeLavorativa -> (nome - chiave primaria)
	nome;
	indirizzo;
	listaImpiegati; -> ogni sede lavorativa almeno un funzionario

Persona -> (codiceFiscale - chiave primaria)
	nome, cognome, codiceFiscale, 
 	mansioneLavorativa, username, password
	login();
	
Impiegato : Persona
	sedeLavorativa
	dataInizio
	dataFine

Funzionari : Impiegato
	aggiungiImpiegato();
	aggiornaImpiegato();

Dirigente : Impiegato
	ricercaImpiegati();
	accediDatiPersone(); -> no amministratori

Amministratori : Dirigente
	@override
	accediDatiPersone(); -> tutto il personale
	
	
	




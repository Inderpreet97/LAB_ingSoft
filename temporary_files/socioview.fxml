<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="300.0" prefWidth="230.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ing_software.circolosportivo_JavaFX_DB.controllers.SocioViewController">
  <children>
      <BorderPane>
         <center>
            <Accordion minHeight="200.0" prefHeight="200.0" BorderPane.alignment="CENTER">
              <panes>
                <TitledPane animated="false" text="Iscrizione Gara o Corso">
                  <content>
                    <AnchorPane>
                           <children>
                              <Label fx:id="sessionLabel" text="Label" />
                           </children>
                        </AnchorPane>
                  </content>
                </TitledPane>
                <TitledPane animated="false" text="Cancella Iscrizione">
                  <content>
                    <AnchorPane />
                  </content>
                </TitledPane>
              </panes>
            </Accordion>
         </center>
         <bottom>
           <Button id="loginButton" fx:id="logoutButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="Logout" BorderPane.alignment="CENTER_RIGHT" />
         </bottom>
      </BorderPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</StackPane>
